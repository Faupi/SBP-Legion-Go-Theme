{
	"name": "Legion Go Theme",
	"author": "azu",
	"target": "System-Wide",
	"version": "v0.22",
	"description": "Theme for the Lenovo Legion Go",
	"manifest_version": 5,
	"inject": {},
	"dependencies": {},
	"patches": {
		"Apply": {
			"type": "dropdown",
			"default": "Legion Go",
			"values": {
				"Xbox/Legion Go": {
					"ps.css": [
						"SP",
						"MainMenu",
						"QuickAccess"
					]
				},
				"Legion Go": {
					"lego.css": [
						"SP",
						"MainMenu",
						"QuickAccess"
					]
				}
			},
			"components": []
		},
		"L is Select": {
			"type": "checkbox",
			"default": "No",
			"values": {
				"Yes": {
					"l_is_select.css": [
						"SP",
						"MainMenu",
						"QuickAccess"
					]
				},
				"No": {}
			},
			"components": []
		},
		"L is Start": {
			"type": "checkbox",
			"default": "No",
			"values": {
				"Yes": {
					"l_is_start.css": [
						"SP",
						"MainMenu",
						"QuickAccess"
					]
				},
				"No": {}
			},
			"components": []
		}
	}
}
